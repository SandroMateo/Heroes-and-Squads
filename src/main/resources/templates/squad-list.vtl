<h1>Choose a Squad To Join!</h1>
#if(!$squadList)
  <h4>there are no squads</h4>
#else
  <form action="/finalist" method="post">
    #foreach($squad.getName() in $squadList)
      <label for=$squad.getName()>
      <input type="radio" name="squadSelect" value=$squad.getId()>
        $squad.getName()
      </label>
    #end
    <button type="submit" class="btn btn-danger">Join!</button>
  </form>

#end
<a href="/squad-form">or create your own squad</a>
